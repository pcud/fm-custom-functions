                );
                ~order = 8; Case (
                    ~value = "label"; "";
                    ~value = "script"; JSONSetElement ( ~json
                        ; ["NAV_KEY"; ""; JSONString]
                    );
                    ~value = "hide"; True
                );
                ~order = 9; Case (
