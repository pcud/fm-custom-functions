                    ~value = "label"; "admin";
                    ~value = "script"; JSONSetElement ( ~json
                        ; ["NAV_KEY"; "admin"; JSONString]
                    );
                    ~value = "hide"; not hasAccessTo ( "MENU.ADMIN" )
                );
                ~order = 5; Case (
                    ~value = "label"; "logs";
                    ~value = "script"; JSONSetElement ( ~json
                        ; ["NAV_KEY"; "logs"; JSONString]
                    );
                    ~value = "hide"; not hasAccessTo ( "MENU.LOGS" )
                );
                ~order = 6; Case (
                    ~value = "label"; "";
                    ~value = "script"; JSONSetElement ( ~json
                        ; ["NAV_KEY"; ""; JSONString]
                    );
                    ~value = "hide"; True
                );
                ~order = 7; Case (
                    ~value = "label"; "";
                    ~value = "script"; JSONSetElement ( ~json
                        ; ["NAV_KEY"; ""; JSONString]
                    );
                    ~value = "hide"; True
                );
                ~order = 8; Case (
